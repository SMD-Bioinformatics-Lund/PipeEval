# Run profiles
[wgs-single]
pipeline = dna-const
profile = wgs,hg38_masked
sample_type = single
samples = hg002
default_panel = "OMIM-AUTO"

[wgs-trio]
pipeline = dna-const
profile = wgs,hg38_masked
sample_type = trio
samples = hg002,hg003,hg004
default_panel = "OMIM-AUTO"

[onco]
pipeline = dna-const
profile = onco,hg38_masked
sample_type = single
samples = seracare
default_panel = "onco_twist_panel"

[oncov1]
pipeline = dna-const
profile = oncov1,hg38_masked
sample_type = single
samples = seracare
default_panel = "onco_twist_panel"

[wgs-han-giab]
pipeline = dna-const
profile = wgs,hg38_masked
sample_type = single
samples = hg005
default_panel = "OMIM-AUTO"

[modycf]
pipeline = dna-const
profile = modycf,hg38_masked
sample_type = single
samples = seracare
default_panel =  "monogen-diabetes"

[myeloid]
pipeline = dna-const
profile = myeloid,hg38_masked
sample_type = single
samples = hg002
default_panel = "arftlig-myeloisk-malignitet"

[rna-test]
pipeline = rna-const
profile = ...
sample_type = single
samples = hg002-rna

# Pipeline settings
[pipeline-default]
start_nextflow_analysis = /fs1/jakob/src/bnf-infrastructure/start_nextflow_analysis.pl
log_base_dir = /mnt/beegfs/nextflow
trace_base_dir = /mnt/beegfs/nextflow/reports
work_base_dir = /mnt/beegfs/nextflow
base = /fs1/results_dev/pipeeval
datestamp = true
queue = grace-lowest
executor = slurm
cluster = grace

[pipeline-dna-const]
repo = /fs1/pipelines/pipeeval/repos/cmd_wgs
baseline_repo = /fs1/pipelines/pipeeval/repos/cmd_wgs_baseline
runscript = main.nf
singularity_version = 3.8.0
nextflow_version = 21.10.6
container = /fs1/resources/containers/wgs_active.sif

[pipeline-rna-const]
repo = /fs1/pipelines/pipeeval/repos/rna_const
baseline_repo = /fs1/pipelines/pipeeval/repos/rna_const_baseline
runscript = main.nf
singularity_version = 3.8.0
nextflow_version = 24.04.3
container = /fs1/resources/containers/wgs_active.sif

