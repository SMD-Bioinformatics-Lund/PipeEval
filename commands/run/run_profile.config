# sample_types is required for more than one sample in a case

[wgs-single]
pipeline = dna-const
profile = wgs,hg38_masked
samples = hg002
default_panel = "OMIM-AUTO"
// Optional override?
csv = ...

[wgs-trio]
pipeline = dna-const
profile = wgs,hg38_masked
samples = hg002,hg003,hg004
sample_types = proband,father,mother
default_panel = "OMIM-AUTO"

[onco]
pipeline = dna-const
profile = onco,hg38_masked
samples = seracare
default_panel = "onco_twist_panel"

[oncov1]
pipeline = dna-const
profile = oncov1,hg38_masked
samples = seracare
default_panel = "onco_twist_panel"

[wgs-han-giab]
pipeline = dna-const
profile = wgs,hg38_masked
samples = hg005
default_panel = "OMIM-AUTO"

[modycf]
pipeline = dna-const
profile = modycf,hg38_masked
samples = seracare
default_panel =  "monogen-diabetes"

[myeloid]
pipeline = dna-const
profile = myeloid,hg38_masked
samples = hg002
default_panel = "arftlig-myeloisk-malignitet"

[rna-test]
pipeline = rna-const
profile = cmd
samples = hg002-rna
csv = ...

[somatic-test]
pipeline = somatic
profile = test
samples = somatic_sample_n,somatic_sample_t
sample_types = N,T
